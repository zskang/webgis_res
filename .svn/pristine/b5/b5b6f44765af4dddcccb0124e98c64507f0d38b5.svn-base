.ui-gis-right-title-bg{ width:100%; font-size:14px; font-weight:bold; background-image:url(images/ui-gis-right-title-bg.jpg); color:#001F32; height:32px;  border-bottom:1px solid #C4E3EF; }
.ui-gis-right-title-text{ background-image:url(images/ui-gis-right-title-icon.png); background-repeat:no-repeat; background-position:5px 35%; text-indent:35px;line-height:32px;  }
.ui-gis-fonter-bg { background-image:url(images/ui-gis-footer-bg.jpg); height:30px; border-top:1px solid #BEE4F8; border-bottom:1px solid #DBECF7;}
a.ui-gis-link:link,a.ui-gis-link:visited{  line-height:24px; color:#399BCD; text-decoration:none; font-weight:bold;}
