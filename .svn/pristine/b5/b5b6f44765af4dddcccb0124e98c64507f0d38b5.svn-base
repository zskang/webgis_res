*{ margin:0px; padding:0px;}
body{ font-size:12px;}
.ui-gis-head{ height:60px; background-image:url(gis_img/ui-gis-head-bg.jpg); background-repeat:repeat-x;}
.ui-gis-system-name{ float:left; margin-left:20px; line-height:60px; color:#FFF; font-weight:bold; background:url(images/logo.png) no-repeat; background-position: 3% center; text-indent:132px; }
.ui-gis-head-right-menu{ float:right; margin-right:20px; margin-top:20px;}
.ui-gis-banner{ height:34px; background-image:url(gis_img/ui-gis-banner-bg.jpg); background-repeat:repeat-x; border-bottom:1px #C2C2C2 solid;}
img{ border:none;}
.ui-gis-legend{ }
.ui-gis-legend-title{ height:24px; font-size:12px; background:url(images/gis-title_icon.gif) no-repeat #71A8D1; background-position: 3% center; text-indent:26px;   line-height:24px; font-weight:bold; color:#FFF;}
.ui-gis-legend-sub_title{ height:24px; background-image:url(images/gis-sub_title_bg.gif); line-height:24px; color:#E56D0D; text-indent:24px; overflow:hidden; }
.ui-gis-legend-sub_title span{  height:24px; background-image:url(images/gis-sub_title_icon.gif);float:left; background-position: 18% center; background-repeat:no-repeat; display:block; }
.ui-gis-legend-sub_title b{  height:24px;   margin-right:5px; float:right;}
.ui-gis-legend-content{ padding:5px; overflow:hidden;}
.ui-gis-legend-left{ float:left; width:49.5%; line-height:22px; border-bottom:1px #EAEAEA solid; border-right:1px #EAEAEA solid; text-indent:5px;  padding-top:5px; overflow:hidden; }
.ui-gis-legend-left span { float:right; margin-right:5px; display:inline-block;     }
.ui-gis-legend-right{ float:right;  width:48%;  text-indent:24px; height:22px;  line-height:22px; border-bottom:1px #EAEAEA solid; text-indent:5px; padding-left:2%;  padding-top:5px; overflow:hidden; }
.ui-gis-legend-right span { float:right; margin-right:5px;  display:inline-block;    }
