html {
	height: 100%
}

body {
	height: 100%;
	font-family: Arial, Helvetica, sans-serif, "宋体";
	font-size: 12px;
	height: 100%;
	color: #404040;
	background: #FFF
}

.ui-form-container {
	margin: 2px;
	border: 0px #99CDE2 solid;
	height: auto;
	overflow: hidden;
}

.ui-form-title {
	font-size: 14px;
	font-weight: bold;
	background-color: #99CDE2;
	line-height: 28px;
	text-indent: 10px
}

.ui-form-input-box {
	width: auto;
	margin: 5px 0px;
	margin-left: 6px;
	overflow: hidden
}

.ui-form-input-box .toolbar {
	margin-left: 70px
}

.ui-form-symbol-color {
	color: #F00
}

.ui-form-input-title {
	display: block;
	float: left;
	width: 120px;
	margin-right: 5px;
	line-height: 25px;
	text-align: right;
	height: 25px;
	font-size: 12px
}

.ui-form-value {
	display: block;
	float: left;
	height: 25px;
	text-align: left;
	font-size: 12px;
}

.ui-form-input-value {
	display: block;
	float: left;
	height: 25px;
	text-align: left;
	font-size: 12px;
}

label {
	margin-right: 4px;
}

.ui-form-input {
	float: left;
	font-size: 12px;
	padding: 3px;
	border: 1px solid #99CDE2;
	color: #797979;
	margin: 2px 0px
}

.ui-form-input:focus {
	background-color: #E7F5FF
}

.ui-form-input-note {
	font-size: 12px;
	padding: 3px;
	border: 1px solid #99CDE2;
	color: #797979;
	margin: 2px 0px
}

.ui-form-input-note:focus {
	background-color: #E7F5FF;
	background-image: url(images/note.png);
	background-repeat: no-repeat;
	background-position: right center
}

* {
