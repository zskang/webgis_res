<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="/common/header.jsp"%>
<html>
	<head>
	<script type="text/javascript">
	In.add('ctcss',{path:'http://${config.webliburl}/cabletech/ui/css/jquery-ct-ui-custom.css'});
	In.add('commonjs', {path : 'http://${config.webliburl}/cabletech/ui/js/jquery.ct.common.js',type : 'js',charset : 'utf-8'});
	In.ready('ctcss','commonjs',function(){
		In.css('.ui-form-input-title {width: 100px;} .ui-form-input-box {width:100%}');
	});
</script>
	</head>
	<body style="width:100%;height:100%;">
		<div class="ui-form-container" style="width:100%;height:100%;">
			<form name="formname" id="formname" method="post" action="">
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">姓名：</span>
				    <span class="ui-form-value">
				    	${person.USERNAME}
				    </span>
				 </div>
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">工号:</span>
				    <span class="ui-form-value">
				    	${person.EMPLOYEENUM}
				    </span>
				 </div>	
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">职位:</span>
				    <span class="ui-form-value">
				    	${person.JOBINFO}
				    </span>
				 </div>				 			 
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">联系电话:</span>
				    <span class="ui-form-value">
				    	${person.PHONE}
				    </span>
				 </div>
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">手机号码:</span>
				    <span class="ui-form-value">
				    	${person.MOBILE}
				    </span>
				 </div>				 
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">维护专业类型:</span>
				    <span class="ui-form-value">
				    	${person.BUSINESSTYPES}
				    </span>
				 </div>			 
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">所属组织:</span>
				    <span class="ui-form-value">
				    	${person.ORGNAME}
				    </span>
				 </div>
				 <div class="ui-form-input-box">
				    <span class="ui-form-input-title">所在区域:</span>
				    <span class="ui-form-value">
				    	${person.REGIONNAME}
				    </span>
				 </div>		 			 				 				 			 				 	 
			</form> 
		</div>
	</body>
</html>
